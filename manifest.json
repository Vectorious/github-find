{
  "name": "github-find",
  "version": "1.0.0",
  "description": "A plugin for searching within Github repos.",
  "homepage_url": "https://github.com/afeld/github-find",
  "background_page": "background.html",
  "page_action": {
    "default_icon": "favicon.ico",
    "default_title": "Press CTL-SHIFT-F to search this repo."
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*/*"],
      "exclude_globs": ["*github.com/*/"],
      "js": ["jquery-1.6.4.min.js", "jquery-ui-1.8.16.position.min.js", "content-keydown.js"]
    }
  ],
  "permissions": ["tabs"]
}
